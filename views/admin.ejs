
<body>
    <div id="app">


    <!-- MAIN --->
    <main class="main main-content">
        <div class="header">
            <h1>Welcome back, Human <%= locals.user.email %> </h1>
            <p><a href="<%= title %>" class="btn btn-primary" role="button"><%= title %></a></p> 
        </div>
        
        <div class="overview-cards">
            <div class="card product-card">
                <div class="title">
                    <h2>Tickets</h2>
                </div>
                <span class="content product-content">
                    <svg>
                   
                    </svg>
                    <div class="number">
                        <tr v-for="ticket in tickets">
                            <td>
                              {{tickets.length }}
                            </td>
                          </tr>
                    </div>
                </span>
            </div>
            <div class="card user-card">
                <div class="title">
                    <h2>Actors</h2>
                </div>
                <span class="content user-content">
                    <svg>
                        
                    </svg>
                    <div class="number">
                        <tr v-for="actor in actors">
                            <td>
                              {{actors.length }}
                            </td>
                          </tr>
                    </div>
                </span>
            </div>
            <div class="card order-card">
                <div class="title">
                    <h2>Locations</h2>
                </div>
                <span class="content order-content">
                    <svg>
                    
                    </svg>
                    <div class="number">
                        <tr v-for="location in locations2">
                            <td>
                              {{locations2.length }}
                            </td>
                          </tr>
                    </div>
                </span>
            </div>
        </div>
    </main>

    
         
</body>
<link rel="stylesheet" href="./admin.css" type="text/css">
</html>

